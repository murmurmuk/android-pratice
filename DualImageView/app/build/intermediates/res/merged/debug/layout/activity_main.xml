<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="@dimen/activity_horizontal_margin"
    android:layout_marginRight="@dimen/activity_horizontal_margin"
    android:layout_marginTop="@dimen/activity_vertical_margin"
    tools:context="murmur.dualimageview.MainActivity">
    <murmur.dualimageview.DualImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:Dual_angel="@drawable/ic_check_box_black_24dp"
        app:Dual_evil="@drawable/ic_check_box_outline_blank_black_24dp"
        />
    <murmur.dualimageview.DualImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        app:Dual_angel="@drawable/ic_radio_button_checked_black_24dp"
        app:Dual_evil="@drawable/ic_radio_button_unchecked_black_24dp"

        />
    <murmur.dualimageview.DualImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        app:Dual_angel="@drawable/ic_star_black_24dp"
        app:Dual_evil="@drawable/ic_star_border_black_24dp"
        />
    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:id="@+id/button"
        android:layout_centerInParent="true"
        android:text="@string/app_name"
        />

</RelativeLayout>
